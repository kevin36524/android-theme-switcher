<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Check the "res/values/attrs.xml" file for where these style names are first defined.  -->
    <style name="Theme.Material_Light" parent="Theme.AppCompat.Light">
        <item name="pageBackground">@style/page_background_wh</item>
        <item name="textSubheader">@style/text_subheader_wh</item>
        <item name="textLarge">@style/text_large_wh</item>
        <item name="textRegular">@style/text_regular_wh</item>
        <item name="whiteBackground">@style/white_background_wh</item>
        <item name="button">@style/button_wh</item>
        <item name="spinner">@style/spinner_wh</item>
        <item name="actionBarStyle">@style/action_bar_wh</item>

        <item name="mycol_primary">@color/orig_primary</item>
        <item name="mycol_primary_dark">@color/orig_primary_dark</item>
        <item name="mycol_primary_light">@color/orig_primary_light</item>
        <item name="mycol_accent">@color/orig_accent</item>
        <item name="mycol_primary_text">@color/orig_primary_text</item>
        <item name="mycol_secondary_text">@color/orig_secondary_text</item>
        <item name="mycol_icons">@color/orig_icons</item>
        <item name="mycol_divider">@color/orig_divider</item>
        <item name="mycol_extraWhite">@color/orig_extraWhite</item>

    </style>

    <style name="Theme.YOUR_CUSTOM_THEME" parent="Theme.Material_Light">
        <!-- Define styles for YOUR_CUSTOM_THEME here. -->
        <item name="mycol_primary">@color/primary</item>
        <item name="mycol_primary_dark">@color/primary_dark</item>
        <item name="mycol_primary_light">@color/primary_light</item>
        <item name="mycol_accent">@color/accent</item>
        <item name="mycol_primary_text">@color/primary_text</item>
        <item name="mycol_secondary_text">@color/secondary_text</item>
        <item name="mycol_icons">@color/icons</item>
        <item name="mycol_divider">@color/divider</item>
        <item name="mycol_extraWhite">@color/extraWhite</item>

    </style>

</resources>
